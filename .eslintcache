        this.props.id,\n        this.state.sort_by,\n        this.state.sort_order\n      ).then((comments) => {\n        this.setState(comments);\n      });\n    }\n  }\n\n  commentAdder = (newComment) => {\n    this.setState((currState) => {\n      return {\n        comments: [...currState.comments, newComment],\n        hasNewComment: true,\n      };\n    });\n  };\n\n  sortComments = (sort, order) => {\n    this.setState((currState) => {\n      return {\n        comments: [...currState.comments],\n        sort_by: sort,\n        sort_order: order,\n      };\n    });\n  };\n\n  commentDeleter = () => {\n    getCommentsByArticleId(\n      this.props.id,\n      this.state.sort_by,\n      this.state.sort_order\n    ).then((comments) => {\n      this.setState(comments);\n    });\n  };\n\n  handleChange = (event) => {\n    const order = event.target.value;\n    this.setState({ sort_order: order });\n  };\n\n  render() {\n    const { loggedInUser } = this.props;\n    return (\n      <div className=\"comments-container\">\n        <AddComment\n          loggedInUser={loggedInUser}\n          articleId={this.props.id}\n          commentAdder={this.commentAdder}\n        ></AddComment>\n        <div className=\"sort-comments\">\n          <Dropdown>\n            <Dropdown.Toggle\n              style={{\n                margin: \"1%\",\n                color: \"white\",\n                background: \"#eb5c44\",\n                border: \"none\",\n              }}\n            >\n              Sort\n            </Dropdown.Toggle>\n            <Dropdown.Menu>\n              <Dropdown.Item>\n                <Button\n                  style={{\n                    color: \"black\",\n                  }}\n                  className=\"sort-button\"\n                  variant=\"outlined\"\n                  onClick={() => this.sortComments(\"author\")}\n                >\n                  Author\n                </Button>\n              </Dropdown.Item>\n              <Dropdown.Item>\n                <Button\n                  style={{\n                    color: \"black\",\n                  }}\n                  variant=\"outlined\"\n                  onClick={() => this.sortComments(\"created_at\")}\n                >\n                  Date\n                </Button>\n              </Dropdown.Item>\n              <Dropdown.Item>\n                <Button\n                  style={{\n                    color: \"black\",\n                  }}\n                  variant=\"outlined\"\n                  onClick={() => this.sortComments(\"votes\")}\n                >\n                  Votes\n                </Button>\n              </Dropdown.Item>\n            </Dropdown.Menu>\n          </Dropdown>\n          <select\n            className=\"select\"\n            onChange={this.handleChange}\n            name=\"order\"\n            id=\"order\"\n          >\n            <option value=\"asc\">ASC</option>\n            <option value=\"desc\">DESC</option>\n          </select>\n        </div>\n        {this.state.comments.length === 0 ? (\n          <h4 style={{ color: \"white\" }}>\n            You're the first here...add a comment to get the discussion started!\n          </h4>\n        ) : (\n          this.state.comments.map((comment) => {\n            return (\n              <CommentCard\n                id={comment.comment_id}\n                voteUpdater={this.voteUpdater}\n                commentDeleter={this.commentDeleter}\n                loggedInUser={loggedInUser}\n                key={comment.comment_id}\n                article_id={this.props.id}\n                comment={comment}\n              ></CommentCard>\n            );\n          })\n        )}\n      </div>\n    );\n  }\n}\n\nexport default Comments;\n","/Users/charlesfoy/northcoders/frontend/nc-news/src/Components/Loading.js",[],"/Users/charlesfoy/northcoders/frontend/nc-news/src/Components/CommentCard.js",[],"/Users/charlesfoy/northcoders/frontend/nc-news/src/Compo